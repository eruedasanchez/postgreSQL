*-- En este ejercicio, queremos crear una funcion que, elimine a todos 
los alumnos cuyo promedio sea mayor que 90. --*

CREATE OR REPLACE FUNCTION eliminar_alumnos_destacados()
RETURNS VOID
AS $BODY$
DECLARE
		
BEGIN
	DELETE FROM alumnos WHERE promedio >= 90;
	RETURN;
END;
$BODY$ LANGUAGE 'plpgsql';

